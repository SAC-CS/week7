import java.util.Arrays;

public class Array
{
public static void main(String[])

{
  int[] aryNums;

  aryNums = new int[6];


  aryNums[0] = 123;
  aryNums[1] = 456;
  aryNums[2] = 479;
  aryNums[3] = 135;
  aryNums[4] = 246;
  aryNums[5] = 135;
 
  Array.sort(aryNums);

int i;
 
for (i=0; i<aryNums.length ; i++)
{

System.out.println("number: %d\n" + aryNums[i]);

}

}

}
